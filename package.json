  "description": "React components that are made of styled native components. Mostly!",
  "main": "dist/index.js",
  "module": "lib-esm/index.js",
  "jsnext:main": "lib-esm/index.js",
  "sideEffects": false,
  "files": [
    "dist",
    "lib-esm"
