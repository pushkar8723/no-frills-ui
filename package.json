{
    "name": "no-frills-ui",
    "version": "0.0.14-rc.2",
    "description": "React components that are made of styled native components. Mostly!",
    "main": "dist/index.js",
    "module": "lib-esm/index.js",
    "jsnext:main": "lib-esm/index.js",
    "types": "lib-esm/index.d.ts",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./lib-esm/index.d.ts",
            "import": "./lib-esm/index.js",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "lib-esm"
    ],
    "scripts": {
        "clean": "shx rm -rf dist lib-esm",
        "build": "NODE_ENV=production tsc --emitDeclarationOnly && rollup -c",
        "build:dev": "NODE_ENV=development tsc --emitDeclarationOnly && rollup -c",
        "build:storybook": "storybook build",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "size": "size-limit",
        "size:why": "size-limit --why",
        "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx",
        "lint:fix": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx --fix",
        "format": "prettier --write \"**/*.{js,jsx,mjs,ts,tsx,html,json,md,mdx,yml,yaml}\"",
        "format:check": "prettier --check \"**/*.{js,jsx,mjs,ts,tsx,html,json,md,mdx,yml,yaml}\"",
        "storybook": "storybook dev -p 6006",
        "prepare": "husky && npm run clean && npm run build"
    },
    "size-limit": [
        {
            "name": "Full Bundle (CJS)",
            "path": "dist/index.js",
            "limit": "25 KB",
            "gzip": true
        },
        {
            "name": "Tree-shaking - Single Component (Button)",
            "path": "lib-esm/index.js",
            "import": "{ Button }",
            "limit": "5 KB",
            "gzip": true
        },
        {
            "name": "Tree-shaking - Multiple Components",
            "path": "lib-esm/index.js",
            "import": "{ Button, Card, Input }",
            "limit": "10 KB",
            "gzip": true
        },
        {
            "name": "Tree-shaking - All exports",
            "path": "lib-esm/index.js",
            "import": "*",
            "limit": "25 KB",
            "gzip": true
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pushkar8723/no-frills-ui.git"
    },
    "keywords": [
        "React"
    ],
    "author": "pushkar8723@gmail.com",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/pushkar8723/no-frills-ui/issues"
    },
    "homepage": "https://github.com/pushkar8723/no-frills-ui#readme",
    "engines": {
        "node": ">=20.0.0"
    },
    "peerDependencies": {
        "@emotion/react": "^11.0.0",
        "@emotion/styled": "^11.0.0",
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    },
    "devDependencies": {
        "@axe-core/react": "^4.11.0",
        "@emotion/react": "^11.0.0",
        "@emotion/styled": "^11.0.0",
        "@rollup/plugin-commonjs": "^29.0.0",
        "@rollup/plugin-node-resolve": "^16.0.3",
        "@rollup/plugin-swc": "^0.4.0",
        "@rollup/plugin-terser": "^0.4.4",
        "@size-limit/preset-small-lib": "^12.0.0",
        "@storybook/addon-docs": "^10.0.2",
        "@storybook/addon-links": "^10.0.2",
        "@storybook/addon-webpack5-compiler-swc": "^4.0.1",
        "@storybook/cli": "^10.0.2",
        "@storybook/react-webpack5": "^10.0.2",
        "@swc/core": "^1.15.4",
        "@swc/plugin-emotion": "^14.0.1",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.1",
        "@types/jest": "^30.0.0",
        "@types/jest-axe": "^3.5.9",
        "@types/node": "^25.0.2",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@typescript-eslint/eslint-plugin": "^8",
        "@typescript-eslint/parser": "^8",
        "eslint": "^8",
        "eslint-config-prettier": "^9",
        "eslint-plugin-import": "^2",
        "eslint-plugin-jsx-a11y": "^6",
        "eslint-plugin-prettier": "^5",
        "eslint-plugin-react": "^7",
        "eslint-plugin-react-hooks": "^4",
        "husky": "^9",
        "jest": "^30.2.0",
        "jest-axe": "^10.0.0",
        "jest-environment-jsdom": "^30.2.0",
        "lint-staged": "^16",
        "prettier": "^3",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "remark-gfm": "^4.0.1",
        "rollup": "^4.52.5",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "shx": "^0.4.0",
        "size-limit": "^12.0.0",
        "storybook": "^10.0.2",
        "ts-jest": "^29.4.6",
        "tslib": "^2.0.0",
        "typescript": "^5.7.2"
    },
    "packageManager": "yarn@4.12.0"
}
